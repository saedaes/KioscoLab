<%- model_class = Consultation -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:fecha_consulta) %>:</strong></dt>
  <dd><%= @consultation.fecha_consulta %></dd>
  <dt><strong><%= model_class.human_attribute_name(:matricula) %>:</strong></dt>
  <dd><%= @consultation.matricula %></dd>
  <dt><strong><%= model_class.human_attribute_name(:id_publicacion) %>:</strong></dt>
  <dd><%= @consultation.id_publicacion %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              consultations_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_consultation_path(@consultation), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              consultation_path(@consultation),
              :method => 'delete',
              :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')),
              :class => 'btn btn-danger' %>
</div>
